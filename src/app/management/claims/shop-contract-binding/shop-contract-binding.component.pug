.panel.panel-default
    .panel-heading((click)="show()")
        a Привязка магазина к указанному договору
        i.fa.pull-right.custom-chevron([ngClass]="{'fa-chevron-down': showPanel, 'fa-chevron-right': !showPanel}")
    .panel-body(*ngIf="showPanel")
        kof-modification-detail([name]="'ID магазина'", [value]="changeSet.shopID")
        kof-modification-detail([name]="'ID договора'", [value]="changeSet.contractID")
        kof-modification-detail([name]="'ID инструмента вывода'", [value]="changeSet.payoutToolID")
