.row
    .col-xs-12
        kof-search-form([searchParams]="searchParams", (onSearch)="onSearch($event)")
.row
    .col-xs-12
        kof-loading([isLoading]="isLoading")
            kof-search-result([invoices]="invoices", [searchParams]="searchParams")
            kof-paginate([size]="totalCount", [limit]="limit", [offset]="offset", [pagesOnScreen]="10", (onChange)="onChangePage($event)")
